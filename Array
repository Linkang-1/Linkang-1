package ARRAY;

public class ARRAY1 {

	public static void main(String[] args) {
	
		int a[]={10,20,30}; // HERE WE INITILIZE THE VALUES OF ARRAY WHICH WE REQUIRED
		
		
		for (int i=0;i<=a.length-1;i++)  // HERE i<=a.length-1 bcos when we take = sign at that time a.length 
			                            //will take 1value more than index value so to avoid exception 
			                            //we have to minus 1 from a.length i.e. [a.length-1] 
			
		{
			System.out.println(a[i]); // IT WILL GIVES US THE OUTPUT LINE BY LINE(EACH ELEMENT ON NEXT LINE)
		}

	}

}
